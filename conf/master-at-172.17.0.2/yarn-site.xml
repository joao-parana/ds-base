<configuration>
  <property>
    <name>yarn.resourcemanager.hostname</name>
    <!-- O contêiner Master deve entrar primeiro para obter o endereço IP 2 na subrede 172.17.0 -->
    <!-- Pode ser também o endereço 192.168.169.2 se estiver configurado dessa forma -->
    <value>172.17.0.2</value>
  </property>
  <property>
    <name>yarn.nodemanager.aux-services</name>
    <value>mapreduce_shuffle</value>
  </property>
</configuration>
